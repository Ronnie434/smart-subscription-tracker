{
  "expo": {
    "name": "Subscribely",
    "sdkVersion": "54.0.0",
    "slug": "smart-subscription-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Composed_Icon Exports/Composed_Icon-iOS-Default-1024x1024@1x.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Composed_Icon Exports/Composed_Icon-iOS-Default-1024x1024@1x.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ronnie39.smartsubscriptiontracker",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app does not track you. This permission is required by Apple but we respect your privacy.",
        "NSCameraUsageDescription": "Camera access is not used by this app.",
        "NSPhotoLibraryUsageDescription": "Photo library access is not used by this app.",
        "NSCalendarsUsageDescription": "Calendar access is not used by this app.",
        "NSRemindersUsageDescription": "Reminders access is not used by this app.",
        "NSLocationWhenInUseUsageDescription": "Location access is not used by this app."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.yourname.smartsubscriptiontracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Composed_Icon Exports/Composed_Icon-iOS-Default-1024x1024@1x.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "smartsubscriptiontracker",
    "description": "Track and manage all your subscriptions in one place. Never miss a renewal date and stay on top of your monthly expenses.",
    "githubUrl": "https://github.com/yourusername/smart-subscription-tracker",
    "runtimeVersion": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "6ba49749-39ac-4332-b0a9-1f6a9f00ee3c"
      }
    },
    "owner": "ronnie39",
    "plugins": [
      "expo-font"
    ]
  }
}
